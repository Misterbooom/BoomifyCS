function pascal(row, col) {
    var result = 1;
    var i = 1;
    while (i <= col) {
        result = result * (row - i + 1) / i;
        i = i + 1;
    }
    return result;
}

var rows = 12;
var i = 0;
while (i < rows) {
    var j = 0;
    var line = '';
    while (j <= i) {
        line = line + parse(pascal(i, j), 'string') + ' ';
        j = j + 1;
    }
    explode(line);
    i = i + 1;
}